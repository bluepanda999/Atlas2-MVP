{
  "name": "atlas2",
  "version": "1.0.0",
  "description": "Atlas2 - Comprehensive CSV to API mapping and upload web application",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration:server": "cd api && npm run test:integration:server",
    "test:e2e:server": "npm run build && npm run preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "docker:build": "docker build -t atlas2-web .",
    "docker:run": "docker run -p 3000:3000 atlas2-web",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:test": "docker-compose -f docker-compose.test.yml up -d",
    "migrate": "./scripts/migrate.sh development",
    "migrate:test": "./scripts/migrate.sh test",
    "migrate:prod": "./scripts/migrate.sh production",
    "seed": "./scripts/seed.sh development",
    "seed:test": "./scripts/seed.sh test",
    "setup": "./scripts/dev-setup.sh",
    "start": "./scripts/start-dev.sh",
    "stop": "./scripts/start-dev.sh",
    "logs": "./scripts/logs.sh",
    "db-shell": "./scripts/db-shell.sh",
    "redis-shell": "./scripts/redis-shell.sh",
    "reset-db": "./scripts/reset-db.sh",
    "monitoring": "./scripts/start-monitoring.sh",
    "health": "./scripts/monitoring-health.sh",
    "clean": "rm -rf dist/ build/ node_modules/ api/node_modules/ worker/node_modules/",
    "clean:docker": "docker-compose -f docker-compose.dev.yml down -v",
    "precommit": "npm run lint && npm run test && npm run type-check"
  },
  "keywords": [
    "csv",
    "api",
    "mapping",
    "upload",
    "web",
    "docker",
    "container"
  ],
  "author": "Ed Chan",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "@types/lodash": "^4.14.202",
    "@types/multer": "^1.4.11",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "playwright": "^1.40.1",
    "@playwright/test": "^1.40.1",
    "jest-environment-node": "^29.7.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "zustand": "^4.4.7",
    "antd": "^5.12.8",
    "@ant-design/icons": "^5.2.6",
    "papaparse": "^5.4.1",
    "axios": "^1.6.2",
    "openapi-typescript": "^6.7.5",
    "openapi-typescript-codegen": "^0.25.0",
    "lodash": "^4.17.21",
    "dayjs": "^1.11.10",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "helmet": "^7.1.0",
    "compression": "^1.7.4"
  }
}